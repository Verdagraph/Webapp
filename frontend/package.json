{
	"name": "verdantech-frontend",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint . --fix",
		"format": "prettier --write .",
		"codegen": "rm -r ./src/lib/codegen/; orval --config ./orval_client.config.ts; orval --config ./orval_zod.config.ts",
		"codegenKubbZodSchema": "rm -r ./src/lib/codegen/; kubb --config kubb.config.ts; orval --config ./orval_client.config.ts"
	},
	"msw": {
		"workerDirectory": [
			"static"
		]
	},
	"devDependencies": {
		"@faker-js/faker": "^8.4.1",
		"@iconify/svelte": "^4.0.2",
		"@melt-ui/pp": "^0.3.2",
		"@melt-ui/svelte": "^0.83.0",
		"@radix-ui/colors": "^3.0.0",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tailwindcss/container-queries": "^0.1.1",
		"@types/eslint": "^8.56.7",
		"@types/node": "^20.14.10",
		"autoprefixer": "^10.4.19",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"msw": "^2.3.1",
		"orval": "^6.31.0",
		"postcss": "^8.4.39",
		"prettier": "^3.1.1",
		"prettier-plugin-merge": "^0.7.0",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"svelte": "^5.0.0-next.1",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.4.5",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0-alpha.20",
		"vite": "^5.0.3"
	},
	"type": "module",
	"dependencies": {
		"@internationalized/date": "^3.5.5",
		"@sveltestack/svelte-query": "^1.6.0",
		"axios": "^1.7.2",
		"bits-ui": "^0.21.12",
		"clsx": "^2.1.1",
		"formsnap": "^1.0.1",
		"lucide-svelte": "^0.416.0",
		"paneforge": "^0.0.5",
		"sveltekit-superforms": "^2.16.1",
		"tailwind-merge": "^2.4.0",
		"tailwind-variants": "^0.2.1",
		"vaul-svelte": "^0.3.2",
		"zod": "^3.23.8"
	}
}
